program urav1;
uses math;
     var x, y:float;
begin
  writeln('Введите x: ');
  readln(x);
if ( (1+ln(sqrt( sqr(x) + 1 ))) * tan(sqr(x)) ) >0 then 

begin
  y:= (exp(sin(x)) * cos( exp(ln(x)*3))) / ( (1+ln(sqrt( sqr(x) + 1 ))) * tan(sqr(x)) ) ;
end
else 
    begin
         writeln('Деление на 0');
    end;



  writeln(y:8:3);

  readln();
end.
